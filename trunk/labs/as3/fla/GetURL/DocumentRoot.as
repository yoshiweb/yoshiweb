package {		import flash.display.Sprite;	import flash.events.MouseEvent;		import net.yoshiweb.net.GetURL;	public class DocumentRoot extends Sprite {		/////////////////////////////////////////////////////////////////				public var btn:Sprite;				/////////////////////////////////////////////////////////////////		// コンストラクタ		public function DocumentRoot() {						btn.buttonMode = true;			btn.addEventListener(MouseEvent.CLICK, _clickHandler);					}		/////////////////////////////////////////////////////////////////		// Event Handler		/////////////////////////////////////////////////////////////////		private function _clickHandler(e:MouseEvent):void {						// 実行			new GetURL("http://www.adobe.com/jp/", "_blank");		}		/////////////////////////////////////////////////////////////////		// public		/////////////////////////////////////////////////////////////////				///////////////////////////////////////////////////////////////		// private		/////////////////////////////////////////////////////////////////		/////////////////////////////////////////////////////////////////	}}